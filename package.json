{
  "name": "react-aria-matome-examples",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.cjs.js",
  "typings": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "biome format --write src",
    "lint": "biome lint src",
    "lint:fix": "biome check --write src",
    "lint:markup": "markuplint \"./src/**/*.{jsx,tsx}\"",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "check": "npm run lint:fix && npm run format"
  },
  "dependencies": {
    "@digital-go-jp/tailwind-theme-plugin": "^0.1.17",
    "react": "^18.3.1",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss-react-aria-components": "^1.1.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@markuplint/jsx-parser": "^4.7.7",
    "@markuplint/react-spec": "^4.5.7",
    "@storybook/addon-a11y": "^8.3.6",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/blocks": "^8.3.6",
    "@storybook/react": "^8.3.6",
    "@storybook/react-vite": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^22.7.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.20",
    "markuplint": "^4.9.4",
    "postcss": "^8.4.47",
    "storybook": "^8.3.6",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "volta": {
    "node": "22.9.0"
  }
}
